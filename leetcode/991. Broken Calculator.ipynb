    "class Solution:\n",
    "    def brokenCalc(self, X: int, Y: int) -> int:\n",
    "        steps = 0\n",
    "        while X != Y:\n",
    "            if X < Y:\n",
    "                Y = Y+1 if Y % 2 else  Y//2\n",
    "                steps += 1\n",
    "            else:\n",
    "                return steps + X-Y\n",
    "        return steps"
   ]
