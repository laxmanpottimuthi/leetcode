    "class Solution:\n",
    "    def climbStairs(self, n: int) -> int:\n",
    "        if n < 4:\n",
    "            return n\n",
    "        dp = [1,2]\n",
    "        for i in range(2,n):\n",
    "            dp.append(dp[i-1] + dp[i-2])\n",
    "        return dp[n-1]"
   ]
