    "class Solution:\n",
    "    def hammingDistance(self, x: int, y: int) -> int:\n",
    "        \n",
    "        xor_result = x ^ y\n",
    "        \n",
    "        hamming_dist = bin(xor_result).count('1')\n",
    "        \n",
    "        return hamming_dist"
   ]
