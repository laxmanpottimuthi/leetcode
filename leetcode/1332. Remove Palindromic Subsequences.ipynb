   "source": [
    "class Solution:\n",
    "    def removePalindromeSub(self, s: str) -> int:\n",
    "        if not s:\n",
    "            return 0\n",
    "        for i in range(len(s)//2):\n",
    "            if s[i] != s[~i]:\n",
    "                return 2\n",
    "        return 1"
   ]
  },
