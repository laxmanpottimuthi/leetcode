    "class Solution:\n",
    "    def findDuplicate(self, nums: List[int]) -> int:\n",
    "        t, h = nums[0], nums[nums[0]]\n",
    "        while t != h: \n",
    "            t, h = nums[t], nums[nums[h]]\n",
    "        t = 0\n",
    "        while t != h: \n",
    "            t, h = nums[t], nums[h]\n",
    "        return t"
   ]
  }
