    "def firstBadVersion(self, n):\n",
    "    low = 1\n",
    "    high = n\n",
    "    while low < high:\n",
    "        mid = (low + high) // 2\n",
    "        if isBadVersion(mid) == True:\n",
    "            high = mid\n",
    "        else:\n",
    "            low = mid+1\n",
    "    return low"
